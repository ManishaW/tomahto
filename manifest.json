{
  "manifest_version": 2,

  "name": "Tomahto",
  "description": "This extension closes random tabs",
  "version": "1.0",
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}